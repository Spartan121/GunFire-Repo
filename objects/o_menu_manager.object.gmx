{
case os_android : dir = dir_android; break;
case os_windows : dir = "";     break;
}

if (!file_exists(dir+"game.ini"))
{
ini_open(dir+"game.ini");
ini_write_real("Inf", "version", 0.4);
ini_write_real("Inf", "Build", 12);
