ds_map_add(global.Engine_Settings,"g-RTShadows",ini_read_real("Graphics","RTShadows",true));
ds_map_add(global.Engine_Settings,"g-Casing",ini_read_real("Graphics","Casing",true));
ds_map_add(global.Engine_Settings,"g-ChrShadows",ini_read_real("Graphics","ChrShadows",true));
